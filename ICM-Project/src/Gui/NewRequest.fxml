<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="613.0" prefWidth="922.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.NewChangeRequestFX">
   <children>
      <AnchorPane prefHeight="86.0" prefWidth="922.0" style="-fx-background-color: #4682b4;">
         <children>
            <JFXButton fx:id="Back" contentDisplay="GRAPHIC_ONLY" layoutX="842.0" layoutY="14.0" onAction="#backWasPressed" prefHeight="50.0" prefWidth="72.0">
               <graphic>
                  <ImageView fitHeight="50.0" fitWidth="50.0" onMouseClicked="#backWasPressed" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@Images/ICM_BACK.png" />
                     </image>
                  </ImageView>
               </graphic></JFXButton>
            <Text fill="GHOSTWHITE" layoutX="210.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text=" Submit New Change Request">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Text>
            <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/ICM_LOGO.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <ImageView fx:id="error" fitHeight="17.0" fitWidth="44.0" layoutX="308.0" layoutY="366.0" visible="false">
         <image>
            <Image url="@Images/required.png" />
         </image>
      </ImageView>
      <JFXTextField fx:id="jfxinfSys" focusColor="#30a73a" labelFloat="true" layoutX="60.0" layoutY="131.0" prefHeight="25.0" prefWidth="454.0" promptText="Information System Responsible" style="-fx-background-color: WHITE; -fx-border-color: gray;" unFocusColor="BLACK" />
      <JFXTextField fx:id="jfxrequestedChange" focusColor="#30a73a" labelFloat="true" layoutX="60.0" layoutY="211.0" prefHeight="25.0" prefWidth="454.0" promptText="Requested Change (Title)" style="-fx-background-color: WHITE; -fx-border-color: gray;" unFocusColor="BLACK" />
      <JFXTextField fx:id="jfxsituation" focusColor="#30a73a" labelFloat="true" layoutX="60.0" layoutY="294.0" prefHeight="25.0" prefWidth="454.0" promptText="Situation Description" style="-fx-background-color: WHITE; -fx-border-color: gray;" unFocusColor="BLACK" />
      <JFXTextField fx:id="jfxreasons" focusColor="#30a73a" labelFloat="true" layoutX="60.0" layoutY="366.0" prefHeight="58.0" prefWidth="454.0" promptText="Change Reasons" style="-fx-background-color: WHITE; -fx-border-color: gray;" unFocusColor="BLACK" />
      <JFXTextField fx:id="jfxnotes" focusColor="#30a73a" labelFloat="true" layoutX="60.0" layoutY="470.0" prefHeight="58.0" prefWidth="454.0" promptText="Notes (Optional)" style="-fx-background-color: WHITE; -fx-border-color: gray;" unFocusColor="BLACK" />
      <Button fx:id="attach" defaultButton="true" layoutX="602.0" layoutY="383.0" mnemonicParsing="false" onAction="#attachWasPressed" prefHeight="25.0" prefWidth="161.0" text="Attach File" />
      <Button fx:id="submit" defaultButton="true" layoutX="602.0" layoutY="468.0" mnemonicParsing="false" onAction="#submitWasPressed" prefHeight="66.0" prefWidth="161.0" text="&#9;Submit&#10; Change Request">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Text fx:id="fileName" layoutX="620.0" layoutY="430.0" strokeType="OUTSIDE" strokeWidth="0.0" />
   </children>
</AnchorPane>
